		<servlet-class>n4j.dk.layers.application.StudentDataServlet</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>studentProfile</servlet-name>
		<url-pattern>/layers</url-pattern>
	</servlet-mapping>

	<servlet>
