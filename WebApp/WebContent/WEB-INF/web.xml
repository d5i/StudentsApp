	</servlet>

	<servlet-mapping>
		<servlet-name>deleteProfile</servlet-name>
		<url-pattern>/delete</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>Profile</servlet-name>
		<servlet-class>n4j.dudu.commons.CreateProfileServlet</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>Profile</servlet-name>
		<url-pattern>/createProfile</url-pattern>
	</servlet-mapping>

	<servlet>
