<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
	version="3.1">
	<display-name>WebApp</display-name>
	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
		<welcome-file>index.htm</welcome-file>
		<welcome-file>index.jsp</welcome-file>
		<welcome-file>default.html</welcome-file>
		<welcome-file>default.htm</welcome-file>
		<welcome-file>default.jsp</welcome-file>
	</welcome-file-list>

	<servlet>
		<servlet-name>Dulari</servlet-name>
		<servlet-class>n4j.dudu.commons.LoginServlet</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>Dulari</servlet-name>
		<url-pattern>/login</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>editDelete</servlet-name>
		<servlet-class>n4j.dk.layers.application.EditDeleteServlet</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>editDelete</servlet-name>
		<url-pattern>/editDelete</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>loginForm</servlet-name>
		<servlet-class>n4j.dk.layers.application.LoginServlet</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>loginForm</servlet-name>
		<url-pattern>/loginForm</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>editProfile</servlet-name>
		<servlet-class>n4j.dk.layers.application.EditProfileServlet</servlet-class>
	</servlet>

