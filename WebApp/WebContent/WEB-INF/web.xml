	</servlet-mapping>

	<servlet>
		<servlet-name>showStudents</servlet-name>
		<servlet-class>n4j.dk.layers.application.ShowAllStudentsServlet</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>showStudents</servlet-name>
		<url-pattern>/showStudents</url-pattern>
	</servlet-mapping>

