		<servlet-name>rememberMe</servlet-name>
		<servlet-class>n4j.dk.layers.application.RememberMeServlet</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>rememberMe</servlet-name>
		<url-pattern>/loginPage</url-pattern>
	</servlet-mapping>


</web-app>